{
    "name": "ldiazjaramillo/google-calendar-laravel",
    "description": "Manage events on a Google Calendar",
    "keywords": [
        "mister.diaz",
        "calendar",
        "google",
        "event",
        "schedule",
        "google-calendar-laravel"
    ],
    "homepage": "https://github.com/ldiazjaramillo/google-calendar-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Diaz",
            "email": "ldiazjaramillo@gmail.com",
            "homepage": "https://ldiazjaramillo.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "google/apiclient": "^2.0",
        "illuminate/support": "^5.2.8",
        "nesbot/carbon": "^1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3.4",
        "orchestra/testbench": "^3.2",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "ldiazjaramillo\\GoogleCalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ldiazjaramillo\\GoogleCalendar\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ldiazjaramillo\\GoogleCalendar\\GoogleCalendarServiceProvider"
            ],
            "aliases": {
                "GoogleCalendar": "ldiazjaramillo\\GoogleCalendar\\GoogleCalendarFacade"
            }
        }
    }
}
